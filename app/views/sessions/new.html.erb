<%= form_tag sessions_path do %>
  Log In
  <div>
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div>
    <%= label_tag :password%>
    <%= password_field_tag :password, nil %>
  </div>
  <div>
    <div><%= link_to "Forgot your password?", new_password_reset_path %></div>
    <div><%= submit_tag "Log In" %></div>
  </div>
<% end %>
